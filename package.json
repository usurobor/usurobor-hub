{
  "name": "cnagent",
  "version": "2.1.7",
  "description": "Coherent Network agent CLI â€” everything runs through cn",
  "keywords": [
    "cn-agent",
    "git-cn",
    "coherence-network",
    "ai-agent",
    "cli"
  ],
  "homepage": "https://github.com/usurobor/cn-agent#readme",
  "bugs": {
    "url": "https://github.com/usurobor/cn-agent/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usurobor/cn-agent.git"
  },
  "bin": {
    "cn": "bin/cn"
  },
  "files": [
    "bin/",
    "tools/dist/"
  ],
  "scripts": {
    "build": "dune build && npm run bundle",
    "bundle": "npm run bundle:inbox && npm run bundle:cn",
    "bundle:inbox": "npx esbuild _build/default/tools/src/inbox/output/tools/src/inbox/inbox.js --bundle --platform=node --outfile=tools/dist/inbox.js",
    "bundle:cn": "npx esbuild _build/default/tools/src/cn/output/tools/src/cn/cn.js --bundle --platform=node --outfile=tools/dist/cn.js",
    "test": "dune runtest"
  },
  "author": "usurobor",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18"
  }
}
